<h1>曲履歴一覧</h1>

<table class="table table-striped table-bordered table-condensed">
  <thead>
  <tr>
    <th>Song</th>
    <th>歌った日時</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
  </thead>

  <tbody>
<% @sing_logs.each do |sing_log| %>
  <tr>
    <td><%= sing_log.song_id %></td>
    <td><%= sing_log.created_at %></td>
    <td><%= link_to 'Show', sing_log %></td>
    <td><%= link_to 'Edit', edit_sing_log_path(sing_log) %></td>
    <td><%= link_to 'Destroy', sing_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
  </tbody>
</table>
