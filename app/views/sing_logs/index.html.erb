<h1>曲履歴一覧</h1>

<table class="table table-striped table-bordered table-condensed">
  <thead>
  <tr>
    <th>曲名</th>
    <th>歌手名</th>
    <th>日時</th>
  </tr>
  </thead>

  <tbody>
<% @sing_logs.each do |sing_log| %>
  <tr>
    <td><%= link_to sing_log.song.name, sing_log.song %></td>
    <td><%= link_to sing_log.song.artist.name, sing_log.song.artist %></td>
    <td><%= sing_log.sang_at %></td>
  </tr>
<% end %>
  </tbody>
</table>

<%= paginate @sing_logs %>
