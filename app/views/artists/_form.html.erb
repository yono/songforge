<%= form_for(artist) do |f| %>
  <%= render 'error_messages', object: f.object %>

  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>
  <div class="actions">
    <%= f.submit class: 'btn btn-outline-primary', data: {disable_with: t('dictionary.form.disable_with')} %>
  </div>
<% end %>
